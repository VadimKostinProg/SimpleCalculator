<Window x:Class="Calc.HistoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Calc"
        mc:Ignorable="d"
        Title="History" Height="399" Width="401" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Icon="/calc_icon.png">
    <Window.Background>
        <SolidColorBrush Color="#FFE4DEDE"/>
    </Window.Background>
    <Grid>
        <Border Margin="20" CornerRadius="10" Background="White">
            <Border.Effect>
                <DropShadowEffect Color="LightGray" BlurRadius="10" ShadowDepth="0"/>
            </Border.Effect>
            <ListView x:Name="History_ListView" Margin="20" BorderThickness="0" FontSize="24"/>
        </Border>
    </Grid>
</Window>
